FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState Boussinesq;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles          1;
        molWeight       50;
    }
    equationOfState
    {
        beta            8e-6;
        rho0            4420;
        T0              300;
    }
    thermodynamics
    {
        Cp              730;
        Hf              0;
    }
    transport
    {
        mu              5e-3;
        Pr              0.12;
		kappa           29;
    }
}

